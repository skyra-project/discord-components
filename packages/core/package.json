	"scripts": {
		"build:ts": "tsc -b src",
		"analyze": "cem analyze --litelement",
		"start": "yarn build:ts && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds\"",
		"build": "yarn build:ts && yarn analyze --exclude dist",
		"prepack": "yarn build:ts && yarn analyze --exclude dist",
		"lint": "eslint --ext .ts --fix",
		"format": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
		"test": "yarn build:ts && wtr --coverage",
		"test:watch": "yarn build:ts && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wtr --watch\"",
		"storybook": "yarn build:ts && yarn analyze --exclude dist && concurrently -k -r \"tsc --watch --preserveWatchOutput\" \"wds -c .storybook/server.mjs\"",
		"storybook:build": "yarn build:ts && yarn analyze --exclude dist && build-storybook"
	},
	"dependencies": {
		"lit": "^2.7.2",
		"tslib": "^2.5.0"
	},
	"devDependencies": {
